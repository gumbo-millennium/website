{
    "name": "gumbo-millennium/website",
    "description": "The Gumbo Millennium website.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "advoor/nova-editor-js": "^3.3",
        "artesaos/seotools": "^1.2",
        "barryvdh/laravel-debugbar": "^3.6",
        "cviebrock/eloquent-sluggable": "^10.0",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.0",
        "ebess/advanced-nova-media-library": "^4.0",
        "eduardokum/laravel-mail-auto-embed": "dev-master",
        "eluceo/ical": "^2.5",
        "endroid/qr-code": "^4.4",
        "enshrined/svg-sanitize": "^0.15",
        "ezyang/htmlpurifier": "^4.16",
        "giggsey/libphonenumber-for-php": "^8.10",
        "google/apiclient": "^2.4",
        "google/google-apiclient-walletobjects": "^1.20220426",
        "guzzlehttp/guzzle": "^7.8",
        "html2text/html2text": "^4.2",
        "irazasyed/telegram-bot-sdk": "^3.3",
        "kris/laravel-form-builder": "^1.40",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.7",
        "laravel/nova": "4.23.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.5",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.6",
        "laulamanapps/izettle-api": "dev-master",
        "league/commonmark": "^2.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/glide-laravel": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mischiefcollective/colorjizz": "^1.0",
        "mollie/laravel-mollie": "^2.14",
        "picqer/php-barcode-generator": "^2.2",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^4.0",
        "sloveniangooner/searchable-select": "^1.3",
        "smalot/pdfparser": "^2.7",
        "spatie/laravel-csp": "^2.4",
        "spatie/laravel-flash": "^1.3",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-model-states": "^2.4",
        "spatie/laravel-pdf": "^1.5",
        "spatie/laravel-permission": "^6.1",
        "spatie/laravel-view-models": "^1.2",
        "ssnepenthe/color-utils": "^0.4.2",
        "symfony/process": "^6.3",
        "symfony/yaml": "^6.3",
        "teamtnt/laravel-scout-tntsearch-driver": "^13.2",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.17",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpunit/phpunit": "^10.0",
        "sabre/vobject": "^4.4"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Migrations\\": "database/migrations/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "cd vendor-bundled && unzip searchable-select.zip"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "@php artisan nova:publish",
            "@php artisan horizon:publish",
            "@php artisan telescope:publish"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate || true"
        ],
        "ide": [
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models"
        ],
        "server": [
            "@php artisan telescope:publish",
            "@php artisan horizon:assets"
        ],
        "format": [
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "yamllint": [
            "jsonschema-lint --schema resources/json/schemas/scheduled-mails.schema.json resources/assets/yaml/scheduled-mails.yaml",
            "jsonschema-lint --schema resources/json/schemas/menu.schema.json resources/yaml/menu.yaml",
            "jsonschema-lint --schema resources/json/schemas/permissions.schema.json resources/yaml/permissions.yaml",
            "jsonschema-lint --schema resources/json/schemas/redirects.schema.json resources/yaml/redirects.yaml",
            "jsonschema-lint --schema resources/json/schemas/grants.schema.json resources/yaml/grants.yaml",
            "jsonschema-lint --schema resources/json/schemas/roles.schema.json resources/yaml/roles.yaml"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts-descriptions": {
        "format": "Format code using php-cs-fixer"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gumbo-millennium/izettle-api.git"
        },
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "package",
            "package": {
                "name": "google/google-apiclient-walletobjects",
                "version": "v1.20220426",
                "homepage":"https://developers.google.com/wallet/generic/resources/libraries#php",
                "dist": {
                    "url": "./vendor-bundled/google-walletobjects-v1-rev_20220426-php.zip",
                    "type": "zip"
                },
                "require": {
                    "google/apiclient": "^1.1 | ^2.0"
                },
                "autoload": {
                    "classmap": [
                        "./"
                    ]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sloveniangooner/searchable-select",
                "version": "1.3.2",
                "dist": {
                    "url": "./vendor-bundled/searchable-select.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-4": {
                        "Sloveniangooner\\SearchableSelect\\": "src/"
                    }
                },
                "extra": {
                    "laravel": {
                        "providers": [
                            "Sloveniangooner\\SearchableSelect\\FieldServiceProvider"
                        ]
                    }
                }
            }
        }
    ]
}
