{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "cviebrock/eloquent-sluggable": "^4.6",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "giggsey/libphonenumber-for-php": "^8.10",
        "gumbo-millennium/laravel-database-encryption": "^0.3.0",
        "guzzlehttp/guzzle": "^6.3",
        "jgrossi/corcel": "^2.6",
        "laravel/framework": "~5.8.0",
        "laravel/horizon": "^1.4",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "^3.0",
        "mischiefcollective/colorjizz": "^1.0",
        "overtrue/laravel-lang": "^3.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.8",
        "smalot/pdfparser": "^0.13.2",
        "spatie/laravel-permission": "^2.23",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@composer dump-autoload -d library/wordpress/plugins/gumbo-millennium"
        ],
        "contribute": [
          "docker-compose build",
          "@composer install --dev",
          "yarn install --dev",
          "@php artisan app:env",
          "yarn run development",
          "@php artisan vendor:publish --tag=horizon-assets",
          "@php artisan telescope:publish",
          "docker-compose up -d",
          "echo \"Waiting for MySQL to get ready\" && sleep 10s",
          "@php artisan migrate:fresh --seed",
          "yarn --cwd=library/wordpress/themes/gumbo-millennium/ install",
          "yarn --cwd=library/wordpress/plugins/gumbo-millennium/ install",
          "@composer install --working-dir=library/wordpress/plugins/gumbo-millennium/ --dev",
          "yarn --cwd=library/wordpress/themes/gumbo-millennium/ build",
          "yarn --cwd=library/wordpress/plugins/gumbo-millennium/ build"
        ],
        "symlink": [
          "ln -fs node_modules/bootstrap/scss ./@bootstrap",
          "ln -fs node_modules/@gumbo/theme-spacial/sass ./@gumbo",
          "ln -fs library/wordpress/plugins/gumbo-millennium ./@wp-plugin",
          "ln -fs library/wordpress/themes/gumbo-millennium ./@wp-theme",
          "ls ./ | grep -E '^@' | tee --append .git/info/exclude"
        ],
        "ide": [
          "@php artisan clear-compiled",
          "@php artisan ide-helper:generate",
          "@php artisan ide-helper:models"
        ],
        "optimize": [
          "yarn --cwd=library/wordpress/themes/gumbo-millennium/ install",
          "yarn --cwd=library/wordpress/themes/gumbo-millennium/ build",
          "rm -rf library/wordpress/themes/gumbo-millennium/node_modules/",

          "yarn --cwd=library/wordpress/plugins/gumbo-millennium/ install",
          "yarn --cwd=library/wordpress/plugins/gumbo-millennium/ build",
          "rm -rf library/wordpress/plugins/gumbo-millennium/node_modules/",

          "@composer install --working-dir=library/wordpress/plugins/gumbo-millennium/ -ao"
        ],
        "server": [
          "@php artisan telescope:publish",
          "@php artisan horizon:assets"
        ],
        "lint": [
          "phpcs",
          "phpmd app,bootstrap,database,public,resources,routes,tests text phpmd.xml"
        ],
        "test": [
          "phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
          "php": "7.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/gumbo-millennium/laravel-database-encryption"
      }
    ]
}
