{
    "name": "gumbo-millennium/website",
    "description": "The Gumbo Millennium website.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4 | ^8.0",
        "advoor/nova-editor-js": "^2.0",
        "artesaos/seotools": "^0.20",
        "barryvdh/laravel-debugbar": "^3.6",
        "benjaminhirsch/nova-slug-field": "^1.2",
        "blade-ui-kit/blade-icons": "^0.5",
        "cviebrock/eloquent-sluggable": "^7.0",
        "darryldecode/cart": "^4.2",
        "david-griffiths/nova-dark-theme": "^1.0",
        "doctrine/dbal": "^2.6",
        "ebess/advanced-nova-media-library": "^3.6",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "enshrined/svg-sanitize": "^0.13",
        "fideloper/proxy": "^4.4",
        "giggsey/libphonenumber-for-php": "^8.10",
        "google/apiclient": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "html2text/html2text": "^4.2",
        "irazasyed/telegram-bot-sdk": "^3.3",
        "kabbouchi/nova-impersonate": "^1.10",
        "kris/laravel-form-builder": "^1.40",
        "laravel/framework": "^7.30",
        "laravel/horizon": "^4.0",
        "laravel/nova": "^3.30",
        "laravel/scout": "^8.6",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "laravelium/sitemap": "^7.0",
        "laulamanapps/izettle-api": "dev-develop",
        "league/commonmark": "^1.5",
        "league/glide-laravel": "^1.0",
        "mischiefcollective/colorjizz": "^1.0",
        "mollie/laravel-mollie": "^2.14",
        "phpoffice/phpspreadsheet": "^1.11",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.8",
        "roelofr/laravel-encryption-cast": "^1.2",
        "sentry/sentry-laravel": "^2.5",
        "sloveniangooner/searchable-select": "^1.3",
        "smalot/pdfparser": "^0.13",
        "spatie/laravel-csp": "^2.4",
        "spatie/laravel-flash": "^1.3",
        "spatie/laravel-medialibrary": "^8.10",
        "spatie/laravel-model-states": "^1.4",
        "spatie/laravel-permission": "^3.3",
        "spatie/laravel-view-models": "^1.2",
        "ssnepenthe/color-utils": "^0.4.2",
        "symfony/process": "^5.4",
        "symfony/yaml": "^5.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^8.0",
        "vyuldashev/nova-permission": "^3.0",
        "whitecube/nova-flexible-content": "^0.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "deployer/deployer": "^6.8",
        "facade/ignition": "^2.17",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Migrations\\": "database/migrations/",
            "Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "install-nova-zip": [
            "git ls-files library/composer/nova | xargs git update-index --assume-unchanged",
            "echo 'library/composer/nova' >> .git/info/exclude",
            "rm -rf library/composer/nova/",
            "unzip nova.zip",
            "mv laravel-nova-* library/composer/nova",
            "touch library/composer/nova/src/aliases.php",
            "composer install"
        ],
        "remove-nova": [
            "git checkout library/composer/nova",
            "git clean -dfx library/composer/nova",
            "git ls-files library/composer/nova | xargs git update-index --no-assume-unchanged",
            "composer install"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=nova-assets || true",
            "@php artisan horizon:publish",
            "@php artisan telescope:publish"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate || true"
        ],
        "contribute": [
            "Composer\\Config::disableProcessTimeout",
            "@php -r \"file_exists('.env') || copy('.env.local', '.env');\"",
            "git submodule update --init",
            "docker-compose pull || true",
            "docker-compose up -d",
            "@composer install --dev",
            "@php artisan app:env --force",
            "@php artisan key:generate",
            "npm clean-install",
            "npm run build",
            "docker-compose up -d",
            "echo \"Waiting for MySQL to get ready\" && sleep 10s",
            "@php artisan migrate:fresh --seed --force",
            "@php artisan db:seed --class=LocalSeeder"
        ],
        "ide": [
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models"
        ],
        "server": [
            "@php artisan telescope:publish",
            "@php artisan horizon:assets"
        ],
        "cs": [
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
        ],
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.18"
        }
    },
    "scripts-descriptions": {
        "install-nova-zip": "Installs Laravel Nova from a nova.zip file in the project root"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gumbo-millennium/laravel-database-encryption"
        },
        {
            "type": "vcs",
            "url": "https://github.com/gumbo-millennium/izettle-api.git"
        },
        {
            "type": "path",
            "url": "./library/composer/nova"
        }
    ]
}
